{
    "name": "nchoquet/dis_framework_php7-4",
    "description": "dependency injection system with php 7.4 version",
    "type": "project",
    "autoload": {
        "psr-4": {
            "main\\": "lib/src/main",
            "classes\\": "lib/src/classes",
            "interfaces\\": "lib/src/interfaces",
            "traits\\": "lib/src/traits",
            "app\\": "src"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Choquet",
            "email": "nicolachoquet06250@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "scripts": {
        "post-autoload-dump": [
            "php lib/scripts/generate_cmd.php",
            "php cmd.php generate:index -p context=api",
            "php cmd.php generate:htaccess",
            "php src/scripts/custom_index.php",
            "php src/scripts/custom_cmd.php"
        ]
    },
    "require": {}
}
